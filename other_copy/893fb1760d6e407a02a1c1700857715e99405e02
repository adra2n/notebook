   "source": [
    "# 获取城市名字及对应的城市码号\n",
    "from connection import ConnectionTask\n",
    "c=ConnectionTask()\n",
    "\n",
    "boluome = c.pmdb\n",
    "\n",
    "chinapay_city_db = boluome.chinapay_city\n",
    "\n",
    "city_db_find_all = chinapay_city_db.find()\n",
    "\n",
    "for cityInfo in city_db_find_all:\n",
    "    # print(cityInfo)\n",
    "    enum = cityInfo['pinyin'].upper();\n",
    "    print('{}(\"{}\",\"{}\")'.format(enum, cityInfo['cityName'], cityInfo['id']))"
   ]
