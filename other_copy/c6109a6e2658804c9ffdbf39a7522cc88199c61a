    "import hashlib\n",
    "import json\n",
    "import time\n",
    "import urllib\n",
    "import random\n",
    "from pprint import pprint\n",
    "import requests\n",
    "\n",
    "from connection import ConnectionTask\n",
    "c=ConnectionTask()\n",
    "\n",
    "KOMOVIE_SERVER = 'http://api.komovie.cn/movie/service'\n",
    "KOMOVIE_CHANNEL_ID = '189'\n",
    "KOMOVIE_MD5_KEY = 'GglrL3WIjp6CUZnj'"
   ]
