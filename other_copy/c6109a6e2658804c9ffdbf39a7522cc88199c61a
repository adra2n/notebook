   "source": [
    "def get_cinema_message_from_city_id(city_id):\n",
    "    \"\"\"传入参数城市ID,调用接口:通过城市获取影院列表,\n",
    "       数据插入数据库dianying_cinema_kou,\n",
    "       并调用get_movie_message_from_city_id()函数\n",
    "    \"\"\"\n",
    "    action = 'cinema_Query'\n",
    "    time_stamp = int(time.time() * 1000)\n",
    "    hstr = urllib.request.quote(\"{action}{city_id}{time_stamp}{md5_key}\".format(\n",
    "        action=action, city_id=city_id, time_stamp=time_stamp, md5_key=KOMOVIE_MD5_KEY))\n",
    "    enc = hashlib.md5(hstr.encode('utf-8')).hexdigest()\n",
    "    post_data = {\"action\": action, \"city_id\": city_id, \"time_stamp\": time_stamp, \"enc\": enc}\n",
    "    headers = {\"channel_id\": KOMOVIE_CHANNEL_ID}\n",
    "    try:\n",
    "        resp = requests.post(KOMOVIE_SERVER, headers=headers, params=post_data)\n",
    "#         return resp\n",
    "    except Exception as e:\n",
    "        try:\n",
    "            raise self.retry(countdown=60)\n",
    "        except exceptions.MaxRetriesExceededError:\n",
    "            return 'cityId:{} MaxRetriesExceededError'.format(city_id)\n",
    "    if resp.status_code != 200:\n",
    "        return 'cityId:{} NoFetchData'.format(city_id)\n",
    "    resp_data = resp.json()\n",
    "    cinema_data = resp_data['cinemas']\n",
    "    return cinema_data"
   ]
  },
