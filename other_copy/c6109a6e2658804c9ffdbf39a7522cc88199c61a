   ],
   "source": [
    "def get_movie_plans_from_cinema_id(cinema_id):\n",
    "    \"\"\"查看排期列表\"\"\"\n",
    "    action = 'plan_Query'\n",
    "    ts = time.time()\n",
    "    time_stamp = int(ts * 1000)\n",
    "    plans_data = {}\n",
    "    hstr = urllib.request.quote(\"{action}{cinema_id}{time_stamp}{md5_key}\".format(\n",
    "        action=action, cinema_id=cinema_id, time_stamp=time_stamp, md5_key=KOMOVIE_MD5_KEY))\n",
    "    enc = hashlib.md5(hstr.encode('utf-8')).hexdigest()\n",
    "    post_data = {\"action\": action,\n",
    "                 \"cinema_id\": cinema_id,\n",
    "                 \"time_stamp\": time_stamp,\n",
    "                 \"enc\": enc}\n",
    "    headers = {\"channel_id\": KOMOVIE_CHANNEL_ID}\n",
    "    resp = requests.post(KOMOVIE_SERVER, headers=headers, params=post_data)\n",
    "    if resp.status_code != 200:\n",
    "        return 'cinemaId:{} NoFetchData'.format(cinema_id)\n",
    "    resp_data = resp.json()\n",
    "    for item in resp_data.get('plans', []):\n",
    "        plans_data.setdefault(item['movieId'], [])\n",
    "        item.pop('cinema', None)\n",
    "        item.pop('movie', None)\n",
    "        plans_data[item['movieId']].append(item)\n",
    "    return plans_data\n",
    "pprint(get_movie_plans_from_cinema_id(5442))"
   ]
  }
