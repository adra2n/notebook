    "def server_mkdir_p(sftp, remote_directory):\n",
    "    \"\"\"Change to this directory, recursively making new folders if needed.\n",
    "    Returns True if any folders were created.\"\"\"\n",
    "    try:\n",
    "        sftp.chdir(remote_directory)  # sub-directory exists\n",
    "    except IOError:\n",
    "        dirname, basename = remote_directory.rstrip('/').rsplit('/', 1)\n",
    "        print(dirname,basename)\n",
    "        server_mkdir_p(sftp, dirname)  # make parent directories\n",
    "        sftp.mkdir(remote_directory)  # sub-directory missing, so created it"
   ]
  },
